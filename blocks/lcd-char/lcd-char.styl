.lcd-char {
    margin: 0 0 1px 1px;
    padding: 0;
    border-spacing: 0;
    border: 1px solid black;
    cursor: pointer;
    &__cell {
        width: 10px;
        height: 10px;
        margin: 0;
        padding: 0;
        &_selected_yes {
            background: black;
        }
    }
    &_size_big &__cell {
        width: 30px;
        height: 30px;
    }
}
